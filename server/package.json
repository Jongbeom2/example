{
  "name": "example-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml --watch",
    "predev": "yarn && yarn kill-port 4200",
    "dev": "concurrently \"nodemon\" \"yarn generate\"",
    "prebuild": "yarn cache clean && rimraf ./build && graphql-codegen --config codegen.yml",
    "build": "tsc && tscpaths --project tsconfig.json --src ./src --out ./build",
    "postbuild": "copyfiles -u 1 \"./src/api/**/*.graphql\" ./build",
    "start": "node --trace-warnings ./build/index.js",
    "clean": "yarn cache clean && rimraf ./logs && rimraf ./build ./src/schema.graphql ./src/types/graphql.d.ts",
    "pretty": "prettier --list-different --ignore-unknown --write ./*.json \"./{src,__tests__,__mocks__}/**/*.{js,jsx,ts,tsx,graphql,gql}\""
  },
  "repository": "git+https://github.com/Jongbeom2/example.git",
  "author": "example",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jongbeom2/example/issues"
  },
  "dependencies": {
    "apollo-server": "^2.18.1",
    "apollo-server-express": "^2.18.1",
    "app-root-path": "^3.0.0",
    "aws-sdk": "^2.906.0",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.1",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "csprng": "^0.1.2",
    "data-utils": "^0.0.2",
    "dataloader": "^2.0.0",
    "deep-equal": "^2.0.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "firebase-admin": "^9.9.0",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.9.3",
    "graphql-tools": "^6.2.3",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.29",
    "mongoose": "^5.12.13",
    "request": "^2.88.2",
    "sharp": "^0.27.1",
    "shortid": "^2.2.15",
    "stream": "^0.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-resolvers": "1.18.0",
    "@types/app-root-path": "^1.2.4",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.7",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.5.5",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^0.0.48",
    "@types/mime-types": "^2.1.0",
    "@types/mongodb": "^3.5.34",
    "@types/mongoose": "5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.14.10",
    "@types/request": "^2.48.5",
    "@types/sharp": "^0.28.0",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
